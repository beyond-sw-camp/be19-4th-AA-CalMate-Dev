<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ateam.calmate.ai.query.mapper.AllergyQueryMapper">
    <select id="findMemberAllergyNames" parameterType="BigInteger" resultType="String">
        SELECT
            a.name
        FROM
            member_allergy ma
                JOIN
            allergy a ON ma.allergy_id = a.id
        WHERE
            ma.member_id = #{memberId}
    </select>
</mapper>