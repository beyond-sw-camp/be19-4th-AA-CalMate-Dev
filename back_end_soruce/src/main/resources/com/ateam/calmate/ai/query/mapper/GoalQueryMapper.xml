<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ateam.calmate.ai.query.mapper.GoalQueryMapper">
    <select id="findMemberGoal" parameterType="BigInteger" resultType="com.ateam.calmate.ai.query.dto.GoalQueryDTO">
        SELECT
            goal_type    AS goalType,
            target_value AS targetValue,
            start_date   AS startDate,
            end_date     AS endDate
        FROM
            goal
        WHERE
            member_id = #{memberId}
    </select>
</mapper>